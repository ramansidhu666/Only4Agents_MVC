<!--Fact Counter-->
<section class="fact-counter" style="background-image:url(../Content/images/parallax/image-1.jpg);">
    <div class="auto-container">
        <div class="row clearfix">
            <div class="counter-outer clearfix">
                <!--Column-->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="icon"><span class="flaticon-briefcase-1"></span></div>
                    <div class="count-outer"><span class="count-text" data-speed="1500" data-stop="450">0</span>+</div>
                    <h4 class="counter-title">Total Projects</h4>
                </article>

                <!--Column-->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="icon"><span class="flaticon-cup-2"></span></div>
                    <div class="count-outer"><span class="count-text" data-speed="1000" data-stop="950">0</span>+</div>
                    <h4 class="counter-title">Total Advisors</h4>
                </article>

                <!--Column-->
                <article class="column counter-column col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-duration="0ms">
                    <div class="icon"><span class="flaticon-people-2"></span></div>
                    <div class="count-outer"><span class="count-text" data-speed="1000" data-stop="600">0</span>+</div>
                    <h4 class="counter-title">Total Clients</h4>
